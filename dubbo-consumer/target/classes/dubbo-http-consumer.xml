<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


    <!--消费方应用名-->
    <dubbo:application name="dubbo-consumer"/>

    <context:property-placeholder location="classpath:zookeeper.properties"/>
    <!-- 使用zookeeper注册中心暴露服务地址 -->
    <dubbo:registry protocol="zookeeper" address="${zookeeper.address}"/><!--transport表示传输协议-->

    <!--设置默认协议-->
    <!--使用http协议-->
    <dubbo:protocol name="http" port="8089"/>

    <!-- 生成远程服务代理，可以和本地bean一样使用demoProviderService  check属性，启动的时候是否检查 一般设置为false 启动的时候不检查-->
    <dubbo:reference id="dubboDefaultTestService" interface="org.github.api.DubboDefaultTestService" version="1.0.0" timeout="150000"
                     loadbalance="leastactive" protocol="http" retries="4" check="true" async="true"/>

</beans>